<h2 class="page-header"><%= product.name %></h2>
<div class="product-details">
  <div>
    <img class="product-cover" src="<%= product.coverImagePath %>">
    <div class="product-details-btn-grid">
      <a class="btn btn-primary" href="/products<%= product.id %>/edit">Edit</a>
      <%- include('../partials/deleteForm', { url: `/products/${product.id}` }) %>
    </div>
  </div>
  <div class="product-details-grid">
    
    <div class="product-details-label">Quantity :</div>
    <div><%= product.quantity %></div>
    <div class="product-details-label">Detail :</div>
    <div><%= product.description %></div>
    <div class="product-details-label">instockAt : </div>
    <div><%= product.instockAt %></div>
    <div class="product-details-label">Category : </div>
    <div><%= product.category %></div>

  </div>
</div>